<table class="table table-striped">
  <thead>
    <tr>
      <th>Fighter to win</th>
      <th>In round</th>
      <th>By</th>
      <th>Points</th>
    </tr>
  </thead>
  <tbody>
  <% @prediction.each do |prediction| %>
    <tr>
      <td><%= image_tag prediction.fighter.picture.url, :width => '150px'%></td>
        <td><%= prediction.round_id %></td>
        <td><%= prediction.how_id %></td>
		<td><%= prediction.points %></td>
     </tr>
  
   <% end %>
</tbody>
</table>